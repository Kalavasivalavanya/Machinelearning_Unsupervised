rm_val"]},{"cell_type":"code","execution_count":null,"id":"c80ae2a8-dd38-4e98-a1df-543d3b66ebe9","metadata":{"id":"c80ae2a8-dd38-4e98-a1df-543d3b66ebe9","outputId":"198af757-53b3-49e5-b329-dab57dcabb49","colab":{"base_uri":"https://localhost:8080/","height":488},"executionInfo":{"status":"ok","timestamp":1680181070869,"user_tz":-120,"elapsed":54,"user":{"displayName":"kalava sivalavanya","userId":"05755290286565672160"}}},"outputs":[{"output_type":"execute_result","data":{"text/plain":["              original  min_max_scaling\n","student                                \n","Akbar                7              0.7\n","Andreas              7              0.7\n","Dónal                8              0.8\n","Hana                 2              0.2\n","Konstantinos         7              0.7\n","Kristina             9              0.9\n","Sabrina              6              0.6\n","Sandrine            10              1.0\n","Sevgi                6              0.6\n","Rias                 8              0.8\n","Ben                  8              0.8\n","Joan                 0              0.0\n","Ranjit               7              0.7"],"text/html":["\n","  <div id=\"df-73bcc298-05f6-4ae6-8b13-e9c7898d1eab\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>original</th>\n","      <th>min_max_scaling</th>\n","    </tr>\n","    <tr>\n","      <th>student</th>\n","      <th></th>\n","      <th></th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>Akbar</th>\n","      <td>7</td>\n","      <td>0.7</td>\n","    </tr>\n","    <tr>\n","  