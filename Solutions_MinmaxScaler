 differences."],"metadata":{"id":"RnyMXrO3T8RM"},"id":"RnyMXrO3T8RM"},{"cell_type":"code","source":["df_custom = df_audio_features[['danceability', 'energy', 'loudness']]\n","#df_sk_pre = MinMaxScaler().fit_transform(df_custom)\n","#df = pd.DataFrame(df_sk_pre, columns=df_custom.columns, index=df_custom.index)\n","#change df_custom to df to get the scalled distance values\n","eucl = pd.DataFrame(pairwise_distances(df_custom),\n","                    index=df_custom.index,\n","                    columns=df_custom.index)\n","eucl\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":713},"id":"4CFYKSn2RoFi","executionInfo":{"status":"ok","timestamp":1691160118527,"user_tz":-120,"elapsed":42,"user":{"displayName":"kalava sivalavanya","userId":"05755290286565672160"}},"outputId":"9cd21e46-0e72-400a-bfc3-3e98fe73076f"},"id":"4CFYKSn2RoFi","execution_count":20,"outputs":[{"output_type":"execute_result","data":{"text/plain":["song_name                                                                My Band                                                                   \\\n","artist                                                                                                                      D12                     \n","song_name                                          artist                                                                                           \n","My Band                                        ... D12                                                         5.960464e-08                         \n","The Real Slim Shady                            ... Eminem                                                      8.867181e-01                         \n","Águas De Março                                 ... Antônio Carlos Jobim                                        1.213253e+01                         \n","The Girl From Ipanema                          ... Stan Getz                                                   1.342738e+01  